<div class="row">
  <div class="col-md">

<div class="d-grid gap-3">

<% ps.forEach(function(ps){ %>
  
  <a href="/ps/<%= ps._id %>" class="col text-center category_link">
    <div class="card" style="max-width: 60rem; background-color: rgba(160, 192, 234, 0.911);">
      <div class="card-body">
          <div class="d-flex">
            <h5 class="card-title p-2">Team Name:</h5>
            <h6 class="card-title p-2"><%=ps.TeamName%></h6>
          </div>
          <div class="d-flex">
              <h6 class="card-subtitle mb-2 text-muted">Problem Statment:</h6>
              <p class="card-text"><%=ps.ps%></p>
          </div>
          <div class="d-flex">
              <h6 class="card-subtitle mb-2 text-muted">Technology:</h6>
              <p class="card-text"><%=ps.Technology%></p>
          </div>
          <div class="d-flex justify-content-md-around">
              <div class="d-flex flex-row">
                  <div class="p-2"><h6>Domain:</h6></div>
                  <div class="p-2"><%=ps.domain%></div>
              </div>
              <div class="d-flex flex-row">
                  <div class="p-2"><h6>Uploaded By:</h6></div>
                  <div class="p-2"><%=ps.Username%></div>
              </div>
              <div class="d-flex flex-row">
                  <div class="p-2"><h6>Team Member:</h6></div>
                  <div class="p-2">3</div>
              </div>
              
  
            </div>

      </div>
    </div>
  
      
  </a>


  
  <% }) %>
</div>
</div>

