                  <option value="Space">Space</option>
                  <option value="Green Earth">Green Earth</option>
                  <option value="Assistive Aid">Assistive Aid</option>
                  <option value="Water Management">Water Management</option>
                  <option value="Swachh Bharat">Swachh Bharat</option>
                  <option value="Education">Education</option>
                  <option value="Environment">Environment</option>
                </select>
            </div>

            <div>
                <label for="Technology">Technology</label>
                      <select name="Technology" data-placeholder="Select Categories" multiple class="chosen-select" id="multiple-label-example" tabindex="8">
                        
                        <option value="Agriculture">Agriculture</option>
                        <option value="Defense">Defense</option>
                        <option value="Health Care">Health Care</option>
                        <option value="Smart City">Smart City</option>
                        <option value="Smart Energy">Smart Energy</option>
                        <option value="Security Systems">Security Systems</option>
                        <option value="Automobile">Automobile</option>
                        <option value="Space">Space</option>
                        <option value="Green Earth">Green Earth</option>
                        <option value="Assistive Aid">Assistive Aid</option>
                        <option value="Water Management">Water Management</option>
                        <option value="Swachh Bharat">Swachh Bharat</option>
                        <option value="Education">Education</option>
                        <option value="Environment">Environment</option>
                      </select>
             </div>

               <div>
                     <label for="Tool">Tool</label>
                      <select name="Tool" data-placeholder="Select Categories" multiple class="chosen-select" id="multiple-label-example" tabindex="8">
                        
                        <option value="Agriculture">Agriculture</option>
                        <option value="Defense">Defense</option>
                        <option value="Health Care">Health Care</option>
                        <option value="Smart City">Smart City</option>
                        <option value="Smart Energy">Smart Energy</option>
                        <option value="Security Systems">Security Systems</option>
                        <option value="Automobile">Automobile</option>
                        <option value="Space">Space</option>
                        <option value="Green Earth">Green Earth</option>
                        <option value="Assistive Aid">Assistive Aid</option>
                        <option value="Water Management">Water Management</option>
                        <option value="Swachh Bharat">Swachh Bharat</option>
                        <option value="Education">Education</option>
                        <option value="Environment">Environment</option>
                      </select>
                   
                </div>

        
    


            <div class="d-flex flex-row align-items-center mb-4">
                <i class="fas fa-lock fa-lg me-3 fa-fw"></i>
                <div class="form-outline flex-fill mb-0">
                  <label class="form-label" for="form3Example4c">Keyword</label>
                  <div class="technologyList">
                    <div class="technologyDiv mb-1">
                  <input type="text" id="form3Example4c" class="form-control" name="Keyword" >
                  </div>
                </div>
                <div class="d-flex justify-content-end mx-1 mb-1 mb-lg-1">
                  <button type="button" class="btn btn-outline-primary" id="addTechnologysBtn"> + </button>
                </div>
                </div>
            </div>


            <div>
                <label for="image">Upload Image</label>
                <input type="file" id="image" 
                       name="image" value="" required>
            </div>
            
            <div>
                <button type="submit">Submit</button>
            </div>
        </form>
